var rectangle = {
	"height": 5,
	"width" : 4,
	"contents": function(){
		var sendBack = "height: "+rectangle.height+", width: "+rectangle.width;
		return(sendBack);
	},
	"perimiter": function(){
		return("perimiter: "+ ((this.width*2)+(this.height*2)));
	}
};
alert(rectangle.height);
var testText = rectangle.contents();
label0.text = testText;