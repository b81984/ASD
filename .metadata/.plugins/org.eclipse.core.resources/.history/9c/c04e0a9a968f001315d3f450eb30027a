var rectangle = {
	"title" : "Square",
	"height": 5,
	"width" : 4,
	"contents": function(){
		var sendBack = "Name:"+rectangle.title+" Height: "+rectangle.height+", Width: "+rectangle.width;
		return(sendBack);
	},
	"perimiter": function(){
		return("perimiter: "+ ((this.width*2)+(this.height*2)));
	}
};
label0.text = rectangle.contents();